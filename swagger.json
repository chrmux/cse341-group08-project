                            "properties": {
                                "photo": {
                                    "type": "string"
                                },
                                "title": {
                                    "type": "string"
                                },
                                "ingredients": {
                                    "type": "string"
                                },
                                "instruction": {
                                    "type": "string"
                                },
                                "creator": {
                                    "type": "array"
                                    
                                },
                                "keywords": {
                                    "type": "string"
                                },
                                "culture": {
                                    "type": "string"
                                },
                                "level": {
                                    "type": "string"
                                },
                                "description": {
                                    "type": "string"
