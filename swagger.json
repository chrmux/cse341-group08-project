                "example": "Pork Adobo"
            },
            "keyword": {
                "type": "string",
                "example": "any"
            },
            "User": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string",
                        "example": "user123"
                    },
                    "roles": {
                        "type": "array",
                        "items": {
                            "type": "string",
                            "enum": [
                                "admin",
                                "user"
                            ],
                            "default": "user"
                        }
                    },
                    "password": {
                        "type": "String",
                        "example": "any"
                    }
                }
            },
            "UserUpdateInput": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string",
                        "example": "new_user123"
                    },
                    "roles": {
                        "type": "array",
                        "items": {
                            "type": "string",
                            "enum": [
                                "admin",
                                "user"
                            ],
                            "default": "user"
                        }
